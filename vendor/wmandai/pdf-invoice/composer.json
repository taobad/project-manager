{
    "name": "wmandai/pdf-invoice",
    "description": "Library to generate PDF invoices - Credits to Attila Fulop",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "invoice",
        "pdf",
        "konekt",
        "workice"
    ],
    "support": {
        "source": "https://github.com/wmandai/pdf-invoice"
    },
    "authors": [{
        "name": "William Mandai",
        "homepage": "https://github.com/wmandai"
    }],
    "require": {
        "php": ">=7.1.3",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "laravel/framework": "^5.8|^6.0|^7.0|^8.0",
        "setasign/tfpdf": "1.32"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Workice\\PdfInvoice\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Workice\\PdfInvoice\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Workice\\PdfInvoice\\InvoicrServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}